{
  "manifest_version": 2,
  "name": "O'Reilly Timeout Popup Auto Clicker",
  "version": "1.0.1",
  "description": "Automatically clicks the OK button on O'Reilly timeout popups to prevent session timeouts.",
  "author": "Rinor Ahmeti",
  "homepage_url": "https://github.com/rinorahmeti/oreilly-timeout-clicker",
  "permissions": [
    "*://event.on24.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://event.on24.com/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "O'Reilly Timeout Popup Auto Clicker"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "oreilly-autoclicker@rinorahmeti.com",
      "strict_min_version": "57.0"
    }
  }
} 